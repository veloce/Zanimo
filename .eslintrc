{
    "rules": {
        "strict": 0,
        "curly": 0,
        "no-mixed-requires": 0,
        "indent": [2, 4],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "camelcase": 0,
        "new-cap": 0,
        "no-new-require": 0,
        "no-underscore-dangle": 0,
        "no-use-before-define": [1, "nofunc"]
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "describe": true,
        "it": true
    }
}
